@Article{Li2018,
  author    = {Li, Chuanhuang and Wu, Yan and Yuan, Xiaoyong and Sun, Zhengjun and Wang, Weiming and Li, Xiaolin and Gong, Liang},
  journal   = {International Journal of Communication Systems},
  title     = {Detection and defense of {DDoS} attack–based on deep learning in {OpenFlow}-based {SDN}},
