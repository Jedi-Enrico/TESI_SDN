@InProceedings{Amaral2016,
  author     = {Amaral, P. and Dinis, J. and Pinto, P. and Bernardo, L. and Tavares, J. and Mamede, H. S.},
  title      = {Machine {Learning} in {Software} {Defined} {Networks}: {Data} collection and traffic classification},
  booktitle  = {2016 {IEEE} 24th {International} {Conference} on {Network} {Protocols} ({ICNP})},
  year       = {2016},
  pages      = {1--5},
  month      = nov,
  abstract   = {Software Defined Networks (SDNs) provides a separation between the control plane and the forwarding plane of networks. The software implementation of the control plane and the built in data collection mechanisms of the OpenFlow protocol promise to be excellent tools to implement Machine Learning (ML) network control applications. A first step in that direction is to understand the type of data that can be collected in SDNs and how information can be learned from that data. In this work we describe a simple architecture deployed in an enterprise network that gathers traffic data using the OpenFlow protocol. We present the data-sets that can be obtained and show how several ML techniques can be applied to it for traffic classification. The results indicate that high accuracy classification can be obtained with the data-sets using supervised learning.},
  doi        = {10.1109/ICNP.2016.7785327},
  file       = {IEEE Xplore Abstract Record:https\://ieeexplore.ieee.org/document/7785327:text/html},
  keywords   = {business communication, learning (artificial intelligence), pattern classification, protocols, software defined networking, telecommunication traffic, software defined network, traffic classification, data collection, SDN, forwarding plane, control plane software implementation, OpenFlow protocol, machine learning network control application, ML network control application, enterprise network, traffic data gathering, supervised learning, Switches, Ports (Computers), Protocols, Software, Conferences, Electronic mail, Software defined Networks, Machine Learning, Data Analysis, Traffic Classification},
  shorttitle = {Machine {Learning} in {Software} {Defined} {Networks}},
}

