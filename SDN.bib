  keywords   = {computer network management, software defined networking, telecommunication network routing, telecommunication traffic, wide area networks, software defined wide-area networking, SDN deployment, wide area networks, computer networks management, data center networks, routing technologies, traffic engineering technologies, video streaming, voice over LTE deployments, Software, Computer architecture, Wide area networks, Routing protocols, Internet},
  shorttitle = {{SDN} in wide-area networks},
}

