  url        = {http://www.sciencedirect.com/science/article/pii/S0045790616302798},
  urldate    = {2020-08-25},
}

@Article{Mendiola2017,
  author   = {Mendiola, Alaitz and Astorga, Jasone and Jacob, Eduardo and Higuero, Marivi},
  journal  = {IEEE Communications Surveys Tutorials},
  title    = {A {Survey} on the {Contributions} of {Software}-{Defined} {Networking} to {Traffic} {Engineering}},
  year     = {2017},
  issn     = {1553-877X},
  number   = {2},
  pages    = {918--953},
  volume   = {19},
  abstract = {Since the appearance of OpenFlow back in 2008, software-defined networking (SDN) has gained momentum. Although there are some discrepancies between the standards developing organizations working with SDN about what SDN is and how it is defined, they all outline traffic engineering (TE) as a key application. One of the most common objectives of TE is the congestion minimization, where techniques such as traffic splitting among multiple paths or advanced reservation systems are used. In such a scenario, this manuscript surveys the role of a comprehensive list of SDN protocols in TE solutions, in order to assess how these protocols can benefit TE. The SDN protocols have been categorized using the SDN architecture proposed by the open networking foundation, which differentiates among data-controller plane interfaces, application-controller plane interfaces, and management interfaces, in order to state how the interface type in which they operate influences TE. In addition, the impact of the SDN protocols on TE has been evaluated by comparing them with the path computation element (PCE)-based architecture. The PCE-based architecture has been selected to measure the impact of SDN on TE because it is the most novel TE architecture until the date, and because it already defines a set of metrics to measure the performance of TE solutions. We conclude that using the three types of interfaces simultaneously will result in more powerful and enhanced TE solutions, since they benefit TE in complementary ways.},
  doi      = {10.1109/COMST.2016.2633579},
  file     = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/ielx7/9739/7936707/07762818.pdf?tp=&arnumber=7762818&isnumber=7936707&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50Lzc3NjI4MTg=:application/pdf},
  keywords = {network interfaces, protocols, software defined networking, telecommunication traffic, software-defined networking, OpenFlow software, traffic engineering, TE, congestion minimization, SDN protocol, data-controller plane interfaces, application-controller plane interfaces, management interfaces, path computation element-based architecture, PCE-based architecture, Protocols, Computer architecture, Optimization, Quality of service, Minimization, Switching circuits, Wide area networks, Software-defined networking, traffic engineering, network resource optimisation, flow granularity},
}

@Article{Ahmad2015,
  author     = {Ahmad, Ijaz and Namal, Suneth and Ylianttila, Mika and Gurtov, Andrei},
  journal    = {IEEE Communications Surveys Tutorials},
  title      = {Security in {Software} {Defined} {Networks}: {A} {Survey}},
  year       = {2015},
  issn       = {1553-877X},
  number     = {4},
  pages      = {2317--2346},
  volume     = {17},
  abstract   = {Software defined networking (SDN) decouples the network control and data planes. The network intelligence and state are logically centralized and the underlying network infrastructure is abstracted from applications. SDN enhances network security by means of global visibility of the network state where a conflict can be easily resolved from the logically centralized control plane. Hence, the SDN architecture empowers networks to actively monitor traffic and diagnose threats to facilitates network forensics, security policy alteration, and security service insertion. The separation of the control and data planes, however, opens security challenges, such as man-in-the middle attacks, denial of service (DoS) attacks, and saturation attacks. In this paper, we analyze security threats to application, control, and data planes of SDN. The security platforms that secure each of the planes are described followed by various security approaches for network-wide security in SDN. SDN security is analyzed according to security dimensions of the ITU-T recommendation, as well as, by the costs of security solutions. In a nutshell, this paper highlights the present and future security challenges in SDN and future directions for secure SDN.},
  doi        = {10.1109/COMST.2015.2474118},
  file       = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/stampPDF/getPDF.jsp?tp=&arnumber=7226783&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50LzcyMjY3ODM/Y2FzYV90b2tlbj0xekRvZ01sRjNmMEFBQUFBOks1eFA0VTdkT211SE56bTIteVVGNEFvVWxPU2VUcU5pMWkwTTBudHAzN1pLaVA0RHZtSTNUT1JfUlBlYlZjSzY3cjhlMVdN:application/pdf},
  keywords   = {computer network security, software defined networking, software defined network security, data planes, network control, network intelligence, network state, global visibility, centralized control plane, SDN architecture, network forensics, security policy alteration, security service insertion, security threats, ITU-T recommendation, Computer security, Communication networks, Computer architecture, Control systems, Software defined networking, Network security, SDN, OpenFlow, Network Security, SDN Security, Application Plane, Control Plane, Data Plane, SDN, OpenFlow, network security, SDN security, application plane, control plane, data plane},
  shorttitle = {Security in {Software} {Defined} {Networks}},
}

@Article{ScottHayward2016,
  author   = {Scott-Hayward, Sandra and Natarajan, Sriram and Sezer, Sakir},
  journal  = {IEEE Communications Surveys Tutorials},
  title    = {A {Survey} of {Security} in {Software} {Defined} {Networks}},
  year     = {2016},
  issn     = {1553-877X},
  number   = {1},
  pages    = {623--654},
  volume   = {18},
  abstract = {The proposition of increased innovation in network applications and reduced cost for network operators has won over the networking world to the vision of software-defined networking (SDN). With the excitement of holistic visibility across the network and the ability to program network devices, developers have rushed to present a range of new SDN-compliant hardware, software, and services. However, amidst this frenzy of activity, one key element has only recently entered the debate: Network Security. In this paper, security in SDN is surveyed presenting both the research community and industry advances in this area. The challenges to securing the network from the persistent attacker are discussed, and the holistic approach to the security architecture that is required for SDN is described. Future research directions that will be key to providing network security in SDN are identified.},
  doi      = {10.1109/COMST.2015.2453114},
  file     = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/stampPDF/getPDF.jsp?tp=&arnumber=7150550&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50LzcxNTA1NTA/Y2FzYV90b2tlbj1xcGM5UVp4c1Q2Z0FBQUFBOmtJdGk3RFgzTVpjQ0dsOWxNZFZieXp1ZEFWQmZ0bmJGQy1GYkpaczR2anJVRGlsenlqUmE5Y1JiOHUwQ3BPMVFwR0N4OGJv:application/pdf},
  keywords = {computer network security, software defined networking, software-defined networking, SDN-compliant hardware, SDN-compliant software, SDN-compliant services, cost reduction, network security architecture, Security, Computer architecture, Industries, Software, Protocols, Monitoring, Tutorials, Software Defined Networking, SDN, Network Security, OpenFlow, Secure SDN Architecture, Software-defined networking, SDN, network security, OpenFlow, secure SDN architecture},
}

@Article{Rawat2017,
  author     = {Rawat, Danda B. and Reddy, Swetha R.},
  journal    = {IEEE Communications Surveys Tutorials},
  title      = {Software {Defined} {Networking} {Architecture}, {Security} and {Energy} {Efficiency}: {A} {Survey}},
  year       = {2017},
  issn       = {1553-877X},
  number     = {1},
  pages      = {325--346},
  volume     = {19},
  abstract   = {Software-defined networking (SDN) is an emerging paradigm, which breaks the vertical integration in traditional networks to provide the flexibility to program the network through (logical) centralized network control. SDN has the capability to adapt its network parameters on the fly based on its operating environment. The decoupled structure of SDN serves as a solution for managing the network with more flexibility and ease. In SDN, the centralized cost effective architecture provides network visibility which helps to achieve efficient resource utilization and high performance. Due to the increasingly pervasive existence of smart programmable devices in the network, SDN provides security, energy efficiency, and network virtualization for enhancing the overall network performance. We present various security threats that are resolved by SDN and new threats that arise as a result of SDN implementation. The recent security attacks and countermeasures in SDN are also summarized in the form of tables. We also provide a survey on the different strategies that are implemented to achieve energy efficiency and network security through SDN implementation. In an effort to anticipate the future evolution of this new paradigm, we discuss the main ongoing research efforts, challenges, and research trends in this area. With this paper, readers can have a more thorough understanding of SDN architecture, different security attacks and countermeasures, and energy efficiency.},
  doi        = {10.1109/COMST.2016.2618874},
  file       = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/stampPDF/getPDF.jsp?tp=&arnumber=7593247&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50Lzc1OTMyNDc/Y2FzYV90b2tlbj0zNkZ5Sk9ha05hMEFBQUFBOjYteFVmaTZaUGVpRUJEYWJWV0pmckd3QmRIcFBvVDVOTVVZNWxFU05KcnduNmdLelh6NlJkNmRJUG1uNVpfb25zT0dyNDFZ:application/pdf},
  keywords   = {computer network security, energy conservation, software defined networking, virtualisation, software defined networking, centralized network control, SDN, network visibility, smart programmable device, energy efficiency, network security, network virtualization, network performance enhancement, security attack, Security, Virtualization, Computer architecture, Control systems, Software, Energy consumption, Servers, Software defined network, network virtualization, energy efficiency, SDN security, OpenFlow network},
  shorttitle = {Software {Defined} {Networking} {Architecture}, {Security} and {Energy} {Efficiency}},
}

@Article{Ali2015,
  author   = {Ali, Syed Taha and Sivaraman, Vijay and Radford, Adam and Jha, Sanjay},
  journal  = {IEEE Transactions on Reliability},
  title    = {A {Survey} of {Securing} {Networks} {Using} {Software} {Defined} {Networking}},
  year     = {2015},
  issn     = {1558-1721},
  month    = sep,
  number   = {3},
  pages    = {1086--1097},
  volume   = {64},
  abstract = {Software Defined Networking (SDN) is rapidly emerging as a new paradigm for managing and controlling the operation of networks ranging from the data center to the core, enterprise, and home. The logical centralization of network intelligence presents exciting challenges and opportunities to enhance security in such networks, including new ways to prevent, detect, and react to threats, as well as innovative security services and applications that are built upon SDN capabilities. In this paper, we undertake a comprehensive survey of recent works that apply SDN to security, and identify promising future directions that can be addressed by such research.},
