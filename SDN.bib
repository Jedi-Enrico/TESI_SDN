@Article{Benamrane2017,
  author     = {Benamrane, Fouad and Ben mamoun, Mouad and Benaini, Redouane},
  journal    = {Computers \& Electrical Engineering},
  title      = {An {East}-{West} interface for distributed {SDN} control plane: {Implementation} and evaluation},
