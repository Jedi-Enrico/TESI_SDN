@Article{Li2015,
  author     = {Li, Yong and Chen, Min},
  journal    = {IEEE Access},
  title      = {Software-{Defined} {Network} {Function} {Virtualization}: {A} {Survey}},
  year       = {2015},
  issn       = {2169-3536},
  pages      = {2542--2553},
  volume     = {3},
  abstract   = {Diverse proprietary network appliances increase both the capital and operational expense of service providers, meanwhile causing problems of network ossification. Network function virtualization (NFV) is proposed to address these issues by implementing network functions as pure software on commodity and general hardware. NFV allows flexible provisioning, deployment, and centralized management of virtual network functions. Integrated with SDN, the software-defined NFV architecture further offers agile traffic steering and joint optimization of network functions and resources. This architecture benefits a wide range of applications (e.g., service chaining) and is becoming the dominant form of NFV. In this survey, we present a thorough investigation of the development of NFV under the software-defined NFV architecture, with an emphasis on service chaining as its application. We first introduce the software-defined NFV architecture as the state of the art of NFV and present relationships between NFV and SDN. Then, we provide a historic view of the involvement from middlebox to NFV. Finally, we introduce significant challenges and relevant solutions of NFV, and discuss its future research directions by different application domains.},
  doi        = {10.1109/ACCESS.2015.2499271},
  file       = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/ielx7/6287639/7042252/07350211.pdf?tp=&arnumber=7350211&isnumber=7042252&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50LzczNTAyMTE=:application/pdf},
  keywords   = {optimisation, software architecture, software defined networking, software-defined network function virtualization, network ossification, network function virtualization, virtual network function, SDN, software-defined NFV architecture, agile traffic steering, Computer architecture, Virtualization, Hardware, Software, Home appliances, Control systems, Servers, Software-defined networks, network function virtualization, middlebox, service chain, network virtualization, Software-defined networks, network function virtualization, middlebox, service chain, network virtualization},
  shorttitle = {Software-{Defined} {Network} {Function} {Virtualization}},
}

@Article{Liang2015,
  author     = {Liang, Chengchao and Yu, F. Richard},
  journal    = {IEEE Communications Surveys Tutorials},
  title      = {Wireless {Network} {Virtualization}: {A} {Survey}, {Some} {Research} {Issues} and {Challenges}},
  year       = {2015},
  issn       = {1553-877X},
  number     = {1},
  pages      = {358--380},
  volume     = {17},
  abstract   = {Since wireless network virtualization enables abstraction and sharing of infrastructure and radio spectrum resources, the overall expenses of wireless network deployment and operation can be reduced significantly. Moreover, wireless network virtualization can provide easier migration to newer products or technologies by isolating part of the network. Despite the potential vision of wireless network virtualization, several significant research challenges remain to be addressed before widespread deployment of wireless network virtualization, including isolation, control signaling, resource discovery and allocation, mobility management, network management and operation, and security as well as non-technical issues such as governance regulations, etc. In this paper, we provide a brief survey on some of the works that have already been done to achieve wireless network virtualization, and discuss some research issues and challenges. We identify several important aspects of wireless network virtualization: overview, motivations, framework, performance metrics, enabling technologies, and challenges. Finally, we explore some broader perspectives in realizing wireless network virtualization.},
  doi        = {10.1109/COMST.2014.2352118},
  file       = {IEEE Xplore Full Text PDF:https\://ieeexplore.ieee.org/stampPDF/getPDF.jsp?tp=&arnumber=6887287&ref=aHR0cHM6Ly9pZWVleHBsb3JlLmllZWUub3JnL2Fic3RyYWN0L2RvY3VtZW50LzY4ODcyODc/Y2FzYV90b2tlbj1CN3ZSMXMxa0o0SUFBQUFBOkJBSE1MZG9nbWZYbV9mWl9fcGMtR2hmZDV1ZmpXQTE1T0JQYVp1cnFZMHhvWXhyb091c3ZzWW9DZ1U3Y3lSOGZzQmxIMzk0:application/pdf},
  keywords   = {mobility management (mobile radio), radio networks, telecommunication computing, virtualisation, wireless network virtualization, radio spectrum resources, wireless network deployment, control signaling, resource discovery, resource allocation, mobility management, network management, nontechnical issues, governance regulations, performance metrics, Virtualization, Wireless networks, Virtual private networks, Indium phosphide, Business, Mobile communication, Wireless network virtualization, abstraction and sharing, isolation, cognitive radio and networks, cloud computing},
